{
    "extends": ["eslint:recommended"],
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "env": { "node": true, "es6": true },
    "rules": {
        "no-console": "off",
        "semi": ["error", "always"],
        "handle-callback-err": "error",
        "no-async-promise-executor": "off",
        "@typescript-eslint/no-explicit-any": "off"
    }
}
