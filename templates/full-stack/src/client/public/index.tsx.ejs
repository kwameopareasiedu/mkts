import React from "react";
import { render } from "react-dom";
import { BrowserRouter } from "react-router-dom";
import { DialogProvider, FlashProvider, LocalStorageProvider } from "react-simple-widgets";
import { App } from "./app";

render(
    <BrowserRouter>
        <LocalStorageProvider>
            <DialogProvider>
                <FlashProvider>
                    <App />
                </FlashProvider>
            </DialogProvider>
        </LocalStorageProvider>
    </BrowserRouter>,
    document.querySelector("#root")
);
